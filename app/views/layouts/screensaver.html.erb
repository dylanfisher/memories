<!DOCTYPE html>
<html id="screensaver" class="screensaver" lang="en">
  <head>
    <%= csp_meta_tag %>
    <%= jquery_include_tag %>
    <%= stylesheet_link_tag 'forest/application_public', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'forest/application_public' %>
  </head>
  <body data-screensaver-path="<%= screensaver_path %>">
    <%= yield %>
  </body>
</html>
